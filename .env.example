# Environment Configuration for OCR Services

# Database Configuration
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# MistralAI Configuration (Recommended for faster processing)
# Get your API key from: https://console.mistral.ai/
VITE_MISTRAL_API_KEY=your_mistral_api_key_here
VITE_MISTRAL_MODEL=mistral-large-latest
VITE_MISTRAL_TIMEOUT=30000
VITE_MISTRAL_MAX_FILE_SIZE=16777216

# Dots.ocr API Configuration
# Local Python backend service (recommended)
VITE_DOTS_OCR_ENDPOINT=http://localhost:5001/parse
VITE_DOTS_OCR_BACKEND_URL=http://localhost:5001

# Alternative: Remote API (if available)
# VITE_DOTS_OCR_ENDPOINT=https://api.dots.ocr.com/v1/process
# VITE_DOTS_OCR_API_KEY=your_dots_ocr_api_key_here

# OCR Processing Settings
VITE_DOTS_OCR_TIMEOUT=60000
VITE_DOTS_OCR_MAX_FILE_SIZE=16777216
VITE_DOTS_OCR_CONFIDENCE_THRESHOLD=0.7
VITE_DOTS_OCR_ENABLE_MOCK=false

# DotsOCR Backend Service Settings
DOTS_OCR_BACKEND_PORT=5001
DOTS_OCR_BACKEND_DEBUG=true
DOTS_OCR_MODEL_PATH=./dots.ocr
DOTS_OCR_MIN_PIXELS=256
DOTS_OCR_MAX_PIXELS=1024

# Application Settings
VITE_APP_NAME=Numerizam
VITE_APP_ENV=development
VITE_APP_URL=http://localhost:5173

# Optional: Custom API endpoints
VITE_API_BASE_URL=http://localhost:8000/api
VITE_AUTH_DOMAIN=your_auth_domain

# Debug settings
VITE_DEBUG_MODE=false
VITE_LOG_LEVEL=info